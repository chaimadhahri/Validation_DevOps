pipeline {
    agent any

    stages {
        stage('GIT') {
            steps {
		echo "Getting Project from Git"
                git url: 'https://github.com/chaimadhahri/Validation_DevOps.git', branch: 'chayma'
            }
        }
         stage('MVN CLEAN INSTALL') {
            steps {
                sh 'mvn clean install'
            }
        }

	stage('JUNIT') {
            steps {
		sh 'mvn test'
              
            }
        }
	
	stage('Package') {
            steps {
		sh 'mvn package'
              
            }
	stage('NEXUS'){
            steps{
                sh "mvn deploy -DskipTests=true"
            }
        }

	

    }
}