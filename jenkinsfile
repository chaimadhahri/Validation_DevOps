pipeline {
    agent any

    stages {
        stage('GIT') {
            steps {
		echo "Getting Project from Git"
                git 'https://github.com/chaimadhahri/Validation_DevOps.git'
            }
        }
        stage('Maven clean') {
            steps {
                sh 'mvn clean'
            }
        }
	 stage('Maven compile') {
            steps {
                sh 'mvn compile'
            }
        }
    }
}